void aEtaBarrelEH()
{
  TCanvas *c2 = new TCanvas("c2", "c2",65,52,525,527);
  c2->Range(-60.25,-0.625,562.25,0.625);
  c2->SetLogx();
  TF1 *faEtaBarrelEH = new TF1("faEtaBarrelEH","[0]+[1]*exp(-x*[3]/[2])",1.,500.);
  faEtaBarrelEH->FixParameter(0,340.583);
  faEtaBarrelEH->FixParameter(1,-340.595);
  faEtaBarrelEH->FixParameter(2,6192.6);
  faEtaBarrelEH->FixParameter(3,0.000373614);
  TF1 * faEtaBarrelEH2 = new TF1("faEtaBarrelEH2","[0]+[1]*exp(-x/[2])",1.,500.);
  faEtaBarrelEH2->FixParameter(0,0.0185555);
  faEtaBarrelEH2->FixParameter(1,-0.0470674);
  faEtaBarrelEH2->FixParameter(2,396.959);

  faEtaBarrelEH->GetYaxis()->SetRangeUser(-1, 1);

  faEtaBarrelEH->SetLineColor(1);
   faEtaBarrelEH->Draw();
  faEtaBarrelEH2->Draw("sames");
 

  TLegend* legends = new TLegend(0.65, 0.2, 0.9, 0.4,"","brNDC"); // the numbers determine the position of the box         
  
  legends->SetFillColor(0);                                                                       
  //legends->SetHeader(stat_name);                                                                  
  // //   legends->SetTextSize(40);                                                                  
  legends->AddEntry(faEtaBarrelEH, "for UL2017 (chi2/Ndf = 182.68)","l");//(name of hist,what you want it called in legend, l=line, p=polymarker, f=boxy thing )                                            
  legends->AddEntry(faEtaBarrelEH2,"for current param (chi2/Ndf = 276.719)","l");                                  
legends->Draw();
 c2->Modified();
 c2->cd();
 c2->SaveAs("faEtaBarrelEH.gif");                                                     
}
