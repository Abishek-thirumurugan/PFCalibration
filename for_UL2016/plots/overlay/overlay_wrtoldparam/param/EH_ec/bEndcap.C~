void bEndcap()
{
  TCanvas *c2 = new TCanvas("c2", "c2",65,52,525,527);
  c2->Range(-60.25,-0.625,562.25,0.625);
  c2->SetLogx();

  TF1 *fbEndcap = new TF1("fbEndcap","[0]+([4]*(x-[5])*exp(-(x*[7])))+(([1]+([2]/sqrt(x)))*exp(-(x^[6]/[3])))",2.,500.);
  fbEndcap->FixParameter(0,-2.0995);
  fbEndcap->FixParameter(1,3.41642);
  fbEndcap->FixParameter(2,2.81066);
  fbEndcap->FixParameter(3,5.41626);
  fbEndcap->FixParameter(4,-0.543298);
  fbEndcap->FixParameter(5,-4.35288);
  fbEndcap->FixParameter(6,-0.0420677);
  fbEndcap->FixParameter(7,0.242091);

  TF1 *fbEndcap2 = new TF1("fbEndcap2","([0]+((([1]+([2]/sqrt(x)))*exp(-(x^[6]/[3])))-([4]*exp(-(x^[7]/[5])))))",2.,500.);
  fbEndcap2->FixParameter(0,-0.974251);
  fbEndcap2->FixParameter(1,1.61733);
  fbEndcap2->FixParameter(2,0.0629183);
  fbEndcap2->FixParameter(3,7.78495);
  fbEndcap2->FixParameter(4,-0.774289);
  fbEndcap2->FixParameter(5,7.81399e-05);
  fbEndcap2->FixParameter(6,0.139116);
  fbEndcap2->FixParameter(7,-4.25551);

  fbEndcap->GetYaxis()->SetRangeUser(-2, 2);

  fbEndcap->SetLineColor(1);
  fbEndcap->Draw();
  fbEndcap2->Draw("sames");

  TLegend* legends = new TLegend(0.65, 0.2, 0.9, 0.4,"","brNDC"); // the numbers determine the position of the box         
  
  legends->SetFillColor(0);                                                                       
  //legends->SetHeader(stat_name);                                                                  
  // //   legends->SetTextSize(40);                                                                  
  legends->AddEntry(fbEndcap, "for UL2017 (chi2/Ndf =  2.13304)","l");//(name of hist,what you want it called in legend, l=line, p=polymarker, f=boxy thing )                                                     
  legends->AddEntry(fbEndcap2,"for old param (chi2/Ndf = 2.57971)","l");                                  
legends->Draw();
 c2->Modified();
 c2->cd();
 c2->SaveAs("fbEndcap.gif");                                                     
}
