void bEtaEndcapEH()
{
  TCanvas *c2 = new TCanvas("c2", "c2",65,52,525,527);
  c2->Range(-60.25,-0.625,562.25,0.625);
  c2->SetLogx();
  TF1 *fbEtaEndcapEH = new TF1("fbEtaEndcapEH","[0]+[1]*exp(-x/[2])",1.,500.);
  fbEtaEndcapEH->FixParameter(0,0.0286728);
  fbEtaEndcapEH->FixParameter(1,0.0428134);
  fbEtaEndcapEH->FixParameter(2,263.774); 
  TF1 *fbEtaEndcapEH2 = new TF1("fbEtaEndcapEH2","[0]+[1]*exp(-x/[2])",1.,500.);
  fbEtaEndcapEH2->FixParameter(0,-0.0192991);
  fbEtaEndcapEH2->FixParameter(1,-0.265);
  fbEtaEndcapEH2->FixParameter(2,80.5502);

  fbEtaEndcapEH->GetYaxis()->SetRangeUser(-1, 1);
  fbEtaEndcapEH->GetXaxis()->SetRangeUser(2., 500.);

  fbEtaEndcapEH->SetLineColor(1);
   fbEtaEndcapEH->Draw();
  fbEtaEndcapEH2->Draw("sames");

  TLegend* legends = new TLegend(0.65, 0.2, 0.9, 0.4,"","brNDC"); // the numbers determine the position of the box         
  
  legends->SetFillColor(0);                                                                       
  //legends->SetHeader(stat_name);                                                                  
  // //   legends->SetTextSize(40);                                                                  
  legends->AddEntry(fbEtaEndcapEH, "for UL2017","l");//(name of hist,what you want it called in legend, l=line, p=polymarker, f=boxy thing )                                                     
  legends->AddEntry(fbEtaEndcapEH2,"for current param","l");                                  
legends->Draw();
 c2->Modified();
 c2->cd();
 c2->SaveAs("fbEtaEndcapEH.gif");                                                     
}
