void aEtaBarrelH()
{
  TCanvas *c2 = new TCanvas("c2", "c2",65,52,525,527);
  c2->Range(-60.25,-0.625,562.25,0.625);
  c2->SetLogx();
  TF1 *  faEtaBarrelH = new TF1("faEtaBarrelH","[0]+[1]*x",1.,500.);
  faEtaBarrelH->FixParameter(0,-0.00651428);
  faEtaBarrelH->FixParameter(1,0.0);
  TF1 * faEtaBarrelH2 = new TF1("faEtaBarrelH2","[0]+[1]*x",1.,500.);
  faEtaBarrelH2->FixParameter(0,0.00434994);
  faEtaBarrelH2->FixParameter(1,-5.16564e-06);

  faEtaBarrelH->GetYaxis()->SetRangeUser(-1, 1);

  faEtaBarrelH->SetLineColor(1);
   faEtaBarrelH->Draw();
  faEtaBarrelH2->Draw("sames");
 

  TLegend* legends = new TLegend(0.65, 0.2, 0.9, 0.4,"","brNDC"); // the numbers determine the position of the box         
  
  legends->SetFillColor(0);                                                                       
  //legends->SetHeader(stat_name);                                                                  
  // //   legends->SetTextSize(40);                                                                  
  legends->AddEntry(faEtaBarrelH, "for UL2017","l");//(name of hist,what you want it called in legend, l=line, p=polymarker, f=boxy thing )                                                     
  legends->AddEntry(faEtaBarrelH2,"for current param","l");                                  
legends->Draw();
 c2->Modified();
 c2->cd();
 c2->SaveAs("faEtaBarrelH.gif");                                                     
}
